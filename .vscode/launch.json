            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "visualizerFile": "/Users/maksim/Library/Application Support/Code/User/workspaceStorage/152e7eb6cf62fb1fec5bb6667bf82b76/tonka3000.qtvsctools/qt.natvis.xml",
            "showDisplayString": true,
            "linux": {
